<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="${relativePackage}.${activityClass}" >

    <Button
        android:id="@+id/Champ2"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/karmasmall" />

    <Button
        android:id="@+id/Champ5"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Champ1"
        android:layout_alignTop="@+id/Champ4"
        android:background="@drawable/leesinsmall" />

    <Button
        android:id="@+id/Champ6"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Champ3"
        android:layout_alignTop="@+id/Champ4"
        android:background="@drawable/lulusmall" />

    <Button
        android:id="@+id/Champ7"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Champ4"
        android:layout_below="@+id/Champ4"
        android:layout_marginTop="30dp"
        android:background="@drawable/varussmall" />

    <Button
        android:id="@+id/Champ4"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Champ2"
        android:layout_marginTop="30dp"
        android:layout_toLeftOf="@+id/Champ3"
        android:background="@drawable/luciansmall" />

    <Button
        android:id="@+id/Champ8"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Champ5"
        android:layout_alignTop="@+id/Champ7"
        android:background="@drawable/syndrasmall" />

    <Button
        android:id="@+id/Champ9"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Champ6"
        android:layout_alignTop="@+id/Champ7"
        android:background="@drawable/vladimirsmall" />

    <Button
        android:id="@+id/Champ1"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/Champ2"
        android:layout_alignBottom="@+id/Champ2"
        android:layout_marginRight="42dp"
        android:layout_toLeftOf="@+id/Champ2"
        android:background="@drawable/aatroxsmall" />

    <Button
        android:id="@+id/Champ3"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/Champ4"
        android:layout_marginLeft="42dp"
        android:layout_toRightOf="@+id/Champ2"
        android:background="@drawable/kaylesmall" />

    <ImageView
        android:contentDescription="@null"
        android:id="@+id/currChamp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:src="@drawable/aatrox" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Champ2"
        android:layout_alignParentTop="true"
        android:contentDescription="@null"
        android:text=""
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/currChamp"
        android:layout_centerHorizontal="true"
        android:contentDescription="@null"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Champ1"
        android:layout_below="@+id/currChamp"
        android:src="@android:drawable/alert_light_frame" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/Champ2"
        android:layout_alignTop="@+id/imageView1"
        android:src="@android:drawable/alert_light_frame" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/Champ3"
        android:layout_alignTop="@+id/imageView2"
        android:src="@android:drawable/alert_light_frame" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/Champ2"
        android:layout_toRightOf="@+id/imageView1"
        android:src="@android:drawable/alert_light_frame" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/Champ2"
        android:layout_toRightOf="@+id/Champ2"
        android:src="@android:drawable/alert_light_frame" />

</RelativeLayout>
